{
  "name": "@hocuspocus/leveldb",
  "description": "hocuspocus persistence driver for LevelDB",
  "version": "1.0.0-alpha.9",
  "homepage": "https://hocuspocus.dev",
  "keywords": [
    "hocuspocus",
    "tiptap",
    "leveldb",
    "yjs"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/hocuspocus-leveldb.js",
  "module": "dist/hocuspocus-leveldb.esm.js",
  "types": "dist/packages/leveldb/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hocuspocus-leveldb.esm.js",
      "require": "./dist/hocuspocus-leveldb.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "y-leveldb": "^0.1.0",
    "yjs": "^13.4.12"
  },
  "publishConfig": {
    "registry": "https://registry.ueber.io"
  }
}
