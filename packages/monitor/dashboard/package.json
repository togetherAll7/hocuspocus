{
  "private": true,
  "name": "@hocuspocus/dashboard",
  "version": "1.0.0-alpha.1",
  "description": "hocuspocus monitoring dashboard",
  "homepage": "https://hocuspocus.dev",
  "keywords": [
    "hocuspocus",
    "tiptap",
    "redis",
    "yjs"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE <= 11",
    "not IE_Mob <= 11"
  ],
  "scripts": {
    "start": "parcel watch --public-url '.' -d dist src/index.html",
    "build": "rm -rf dist && parcel build --public-url '.' --no-source-maps -d dist src/index.html"
  },
  "dependencies": {
    "autoprefixer": "^9.7.3",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.26",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.8.10",
    "tailwindcss-font-inter": "^1.0.8",
    "vue": "^2.6.12"
  },
  "devDependencies": {
  }
}
