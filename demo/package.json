{
  "name": "@hocuspocus/demo",
  "version": "0.1.4",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm --experimental-specifier-resolution=node ./index.ts",
    "start:express": "node --loader ts-node/esm --experimental-specifier-resolution=node ./express.ts",
    "start:development": "nodemon -e ts --watch ./ --watch ../packages --exec npm start",
    "start:development:express": "nodemon -e ts --watch ./ --watch ../packages --exec npm run start:express"
  },
  "devDependencies": {
    "@hocuspocus/leveldb": "^1.0.0-alpha.9",
    "@hocuspocus/redis": "^1.0.0-alpha.3",
    "@hocuspocus/server": "^1.0.0-alpha.10",
    "@types/express": "^4.17.9",
    "@types/express-ws": "^3.0.0",
    "@types/node": "^14.14.20",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1"
  }
}
